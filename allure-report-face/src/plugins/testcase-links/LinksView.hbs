{{#if hasLinks}}
    <h3 class="pane__section-title">Links</h3>
    {{#each testIds}}
        <span class="testcase-link">
            <span class="fa fa-database"></span>
            <a class="link" href="{{this.url}}" target="_blank">{{this.name}}</a></span>
    {{/each}}
    {{#each issues}}
        <span class="testcase-link">
            <span class="fa fa-bug"></span>
            <a class="link" href="{{this.url}}" target="_blank">{{this.name}}</a></span>
    {{/each}}

{{/if}}
